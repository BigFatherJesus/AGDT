# OpenAI API Key - Replace 'sk-proj------' with your actual OpenAI API key
OPENAI_API_KEY=sk-proj------

# Input Folder ID of the Google Drive - Replace with the ID of the folder where input files are stored
INPUT_FOLDER_ID=[INPUT FOLDER ID OF THE GOOGLE DRIVE]

# Output Folder ID of the Google Drive - Replace with the ID of the folder where output files will be stored
OUTPUT_FOLDER_ID=[OUTPUT FOLDER ID OF THE GOOGLE DRIVE]

# Service Account JSON file for Google Drive API authentication
SERVICE_ACCOUNT_FILE=credentials.json

# System Prompt - This could be a prompt or initial text used in your application
SYSTEM_PROMPT=
